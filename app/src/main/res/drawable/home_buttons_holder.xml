    android:shape="rectangle"   >

    <gradient
        android:startColor="#06beb6"
        android:endColor="#48b1bf"
        android:type="linear"/>


    <stroke
        android:width="1dp"
        android:color="#E6E6E7" >
    </stroke>

    <padding
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp"    >
    </padding>

    <corners
        android:radius="12dp"   >
    </corners>

</shape>
